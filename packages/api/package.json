    "zod": "^3.24.1"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.1.0",
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.18",
    "@vitest/coverage-v8": "^2.1.8",
    "jsdom": "^25.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
